#include <stdio.h>

int main() {
 char ch;
    printf("enter a char\n");
    scanf("%s",&ch);
    if ((ch>='a'&&ch>='z')||(ch<='A'&&ch>='Z')){
        if((ch=='a'||ch=='e' ch=='i'||ch=='o'||ch=='u')(ch=='A'||ch=='E'||ch=='I'||ch=='O'||ch=='U')){
            printf("ch is vowel\n",ch);
        }
        else{
            printf("ch is consonant\n",ch);
        }
    }

}
